<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ddit.prod.dao.OthersDAO">
	<select id="selectLprodList" resultType="hashMap">
		SELECT LPROD_NM "lprodNm", LPROD_GU "lprodGu"
<!-- 		컬럼명이 키, 그 값이 밸류 -->
		FROM LPROD
	</select>
	<select id="selectBuyerList" parameterType="PagingVO" resultType="BuyerVO">
		SELECT BUYER_ID "buyerId", BUYER_NAME "buyerName", BUYER_LGU "buyerLgu"
		FROM BUYER
		
	</select>
</mapper>
